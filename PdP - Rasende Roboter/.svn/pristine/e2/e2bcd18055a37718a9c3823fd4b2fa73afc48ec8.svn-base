package solver;

import java.util.HashMap;
import java.util.Map;
import java.util.Stack;

public class StructTree {
	public Map<String, Integer> possibilities;
	public Map<String, String> parent;
	public Stack<String> roughStack;
	public Stack<String> invertStack;
	
	public StructTree() {
		possibilities = new HashMap<String, Integer>();
		parent = new HashMap<String, String>();
		roughStack = new Stack<String>();
		invertStack = new Stack<String>();
	}
	
	public void clear() {
		possibilities.clear();
		parent.clear();
		roughStack.clear();
		invertStack.clear();
	}
}
