package model;

/**
 * Class containing the data of a player (points, number of move, ip adress...)
 */
public class Player {
	private int points;
	private int proposedMoves;
	private int movementsDone;
	
	/**
	 * Default Constructor
	 */
	public Player()
	{
		this.points = 0;
		this.proposedMoves = 0;
		this.movementsDone = 0;
	}

	public int getPoints() {
		return points;
	}

	public void setPoints(int points) {
		this.points = points;
	}

	public int getProposedMoves() {
		return proposedMoves;
	}

	public void setProposedMoving(int nb) {
		this.proposedMoves = nb;
	}

	public int getMovementsDone() {
		return movementsDone;
	}

	public void setMovementsDone(int movementsDone) {
		this.movementsDone = movementsDone;
	}

}
