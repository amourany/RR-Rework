package model;

public class Player {
	
	private int points;
	private int proposedMoving;
	
	public Player()
	{
		this.points = 0;
		this.proposedMoving = 0;
	}

	public int getPoints() {
		return points;
	}

	public void setPoints(int points) {
		this.points = points;
	}

	public int getProposedMoving() {
		return proposedMoving;
	}

	public void setProposedMoving(int proposedMoving) {
		this.proposedMoving = proposedMoving;
	}

}
